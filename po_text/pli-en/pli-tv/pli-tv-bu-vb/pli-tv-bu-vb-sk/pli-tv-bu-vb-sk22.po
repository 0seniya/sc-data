msgctxt "pli-tv-bu-vb-sk22:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk22:2.1"
msgid "Mahāvibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk22:3.1"
msgid "Sekhiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk22:4.1"
msgid "Khambhakatavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-sk22:5.1"
msgid "22. Dutiyakhambhakatasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bu-vb-sk22:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-sk22:6.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū khambhakatā antaraghare nisīdanti …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk22:6.3"
msgid "pe …."
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc2
msgctxt "pli-tv-bu-vb-sk22:7.1"
msgid "“Na khambhakato antaraghare nisīdissāmīti sikkhā karaṇīyā”ti."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc3
msgctxt "pli-tv-bu-vb-sk22:8.1"
msgid "Na khambhakatena antaraghare nisīditabbaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-sk22:8.2"
msgid "Yo anādariyaṃ paṭicca ekato vā ubhato vā khambhaṃ katvā antaraghare nisīdati, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bu-vb-sk22:9.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-sk22:9.2"
msgid "asañcicca, assatiyā, ajānantassa, gilānassa, vāsūpagatassa, āpadāsu, ummattakassa, ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc5
msgctxt "pli-tv-bu-vb-sk22:10.1"
msgid "Dutiyasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
