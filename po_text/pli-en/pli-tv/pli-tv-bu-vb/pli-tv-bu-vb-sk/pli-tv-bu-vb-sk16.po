msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-sk16:5.1"
msgid "16. Dutiyakāyappacālakasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bu-vb-sk16:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-sk16:6.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū kāyappacālakaṃ antaraghare nisīdanti, kāyaṃ olambentā …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk16:6.3"
msgid "pe …."
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc2
msgctxt "pli-tv-bu-vb-sk16:7.1"
msgid "“Na kāyappacālakaṃ antaraghare nisīdissāmīti sikkhā karaṇīyā”ti."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc3
msgctxt "pli-tv-bu-vb-sk16:8.1"
msgid "Na kāyappacālakaṃ antaraghare nisīditabbaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-sk16:8.2"
msgid "Kāyaṃ paggahetvā nisīditabbaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-sk16:8.3"
msgid "Yo anādariyaṃ paṭicca kāyappacālakaṃ antaraghare nisīdati kāyaṃ olambento, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bu-vb-sk16:9.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-sk16:9.2"
msgid "asañcicca, assatiyā, ajānantassa, gilānassa, vāsūpagatassa, āpadāsu, ummattakassa, ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc5
msgctxt "pli-tv-bu-vb-sk16:10.1"
msgid "Chaṭṭhasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
