{
  "an1.258:0.1": "Aṅguttara Nikāya 1 ",
  "an1.258:0.2": "20. Sattamavagga ",
  "an1.258:0.3": "258–267 ",
  "an1.258:1.1": "“Etadaggaṁ, bhikkhave, mama sāvikānaṁ upāsikānaṁ paṭhamaṁ saraṇaṁ gacchantīnaṁ yadidaṁ sujātā seniyadhītā. <span class='num'>1</span>",
  "an1.259:1.1": "… Dāyikānaṁ yadidaṁ visākhā migāramātā. <span class='num'>2</span>",
  "an1.260:1.1": "… Bahussutānaṁ yadidaṁ khujjuttarā. <span class='num'>3</span>",
  "an1.261:1.1": "… Mettāvihārīnaṁ yadidaṁ sāmāvatī. <span class='num'>4</span>",
  "an1.262:1.1": "… Jhāyīnaṁ yadidaṁ uttarānandamātā. <span class='num'>5</span>",
  "an1.263:1.1": "… Paṇītadāyikānaṁ yadidaṁ suppavāsā koliyadhītā. <span class='num'>6</span>",
  "an1.264:1.1": "… Gilānupaṭṭhākīnaṁ yadidaṁ suppiyā upāsikā. <span class='num'>7</span>",
  "an1.265:1.1": "… Aveccappasannānaṁ yadidaṁ kātiyānī. <span class='num'>8</span>",
  "an1.266:1.1": "… Vissāsikānaṁ yadidaṁ nakulamātā gahapatānī. <span class='num'>9</span>",
  "an1.267:1.1": "… Anussavappasannānaṁ yadidaṁ kāḷī upāsikā kuraragharikā”ti. <span class='num'>10</span>",
  "an1.267:1.2": "vaggo sattamo. ",
  "an1.267:1.3": "(Etadaggavaggo niṭṭhito.) "
}