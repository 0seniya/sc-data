{
  "an1.219:0.1": "Aṅguttara Nikāya 1 ",
  "an1.219:0.2": "17. Catutthavagga ",
  "an1.219:0.3": "219–234 ",
  "an1.219:1.1": "“Etadaggaṁ, bhikkhave, mama sāvakānaṁ bhikkhūnaṁ bahussutānaṁ yadidaṁ ānando. <span class='counter' title='counter'>1</span>",
  "an1.220:1.1": "… Satimantānaṁ yadidaṁ ānando. <span class='counter' title='counter'>2</span>",
  "an1.221:1.1": "… Gatimantānaṁ yadidaṁ ānando. <span class='counter' title='counter'>3</span>",
  "an1.222:1.1": "… Dhitimantānaṁ yadidaṁ ānando. <span class='counter' title='counter'>4</span>",
  "an1.223:1.1": "… Upaṭṭhākānaṁ yadidaṁ ānando. <span class='counter' title='counter'>5</span>",
  "an1.224:1.1": "… Mahāparisānaṁ yadidaṁ uruvelakassapo. <span class='counter' title='counter'>6</span>",
  "an1.225:1.1": "… Kulappasādakānaṁ yadidaṁ kāḷudāyī. <span class='counter' title='counter'>7</span>",
  "an1.226:1.1": "… Appābādhānaṁ yadidaṁ bākulo. <span class='counter' title='counter'>8</span>",
  "an1.227:1.1": "… Pubbenivāsaṁ anussarantānaṁ yadidaṁ sobhito. <span class='counter' title='counter'>9</span>",
  "an1.228:1.1": "… Vinayadharānaṁ yadidaṁ upāli. <span class='counter' title='counter'>10</span>",
  "an1.229:1.1": "… Bhikkhunovādakānaṁ yadidaṁ nandako. <span class='counter' title='counter'>11</span>",
  "an1.230:1.1": "… Indriyesu guttadvārānaṁ yadidaṁ nando. <span class='counter' title='counter'>12</span>",
  "an1.231:1.1": "… Bhikkhuovādakānaṁ yadidaṁ mahākappino. <span class='counter' title='counter'>13</span>",
  "an1.232:1.1": "… Tejodhātukusalānaṁ yadidaṁ sāgato. <span class='counter' title='counter'>14</span>",
  "an1.233:1.1": "… Paṭibhāneyyakānaṁ yadidaṁ rādho. <span class='counter' title='counter'>15</span>",
  "an1.234:1.1": "… Lūkhacīvaradharānaṁ yadidaṁ mogharājā”ti. <span class='counter' title='counter'>16</span>",
  "an1.234:1.2": "Vaggo catuttho. "
}