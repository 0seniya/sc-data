{
  "an1.235:0.1": "Aṅguttara Nikāya 1 ",
  "an1.235:0.2": "18. Pañcamavagga ",
  "an1.235:0.3": "235–247 ",
  "an1.235:1.1": "“Etadaggaṁ, bhikkhave, mama sāvikānaṁ bhikkhunīnaṁ rattaññūnaṁ yadidaṁ mahāpajāpatigotamī. <span class='num'>1</span>",
  "an1.236:1.1": "… Mahāpaññānaṁ yadidaṁ khemā. <span class='num'>2</span>",
  "an1.237:1.1": "… Iddhimantīnaṁ yadidaṁ uppalavaṇṇā. <span class='num'>3</span>",
  "an1.238:1.1": "… Vinayadharānaṁ yadidaṁ paṭācārā. <span class='num'>4</span>",
  "an1.239:1.1": "… Dhammakathikānaṁ yadidaṁ dhammadinnā. <span class='num'>5</span>",
  "an1.240:1.1": "… Jhāyīnaṁ yadidaṁ nandā. <span class='num'>6</span>",
  "an1.241:1.1": "… Āraddhavīriyānaṁ yadidaṁ soṇā. <span class='num'>7</span>",
  "an1.242:1.1": "… Dibbacakkhukānaṁ yadidaṁ bakulā. <span class='num'>8</span>",
  "an1.243:1.1": "… Khippābhiññānaṁ yadidaṁ bhaddā kuṇḍalakesā. <span class='num'>9</span>",
  "an1.244:1.1": "… Pubbenivāsaṁ anussarantīnaṁ yadidaṁ bhaddā kāpilānī. <span class='num'>10</span>",
  "an1.245:1.1": "… Mahābhiññāpattānaṁ yadidaṁ bhaddakaccānā. <span class='num'>11</span>",
  "an1.246:1.1": "… Lūkhacīvaradharānaṁ yadidaṁ kisāgotamī. <span class='num'>12</span>",
  "an1.247:1.1": "… Saddhādhimuttānaṁ yadidaṁ siṅgālakamātā”ti. <span class='num'>13</span>",
  "an1.247:1.2": "Vaggo pañcamo. "
}